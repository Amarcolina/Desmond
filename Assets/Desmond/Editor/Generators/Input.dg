#Events/OnKeyDown

$in UnityEngine.KeyCode key

$method onUpdateEvent static Update
	if(Input.GetKeyDown(<key>)){
		->onKeyDown
	}

#Events/OnKeyUp

$in UnityEngine.KeyCode key

$method onUpdateEvent static Update
	if(Input.GetKeyUp(<key>)){
		->onKeyUp
	}
	
#Events/OnKeyHold

$in UnityEngine.KeyCode key

$method onUpdateEvent static Update
	if(Input.GetKey(<key>)){
		->onKeyHold
	}
	